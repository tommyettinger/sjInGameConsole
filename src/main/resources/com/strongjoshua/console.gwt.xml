<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN"
        "https://www.gwtproject.org/doctype/2.11.0/gwt-module.dtd">
<!-- To compile to Gwt, you need to reference this file in your master .gwt.xml
     file (usually called GdxDefinition.gwt.xml) as follows:

     <inherits name="com.strongjoshua.console" />
  -->
<module>
    <!-- This relative path points to the folder that has sources in it.
         This path is slash-separated and is relative to this file. -->
    <source path="console"/>
    <inherits name="com.badlogic.gdx.backends.gdx_backends_gwt" />

	<extend-configuration-property name="gdx.reflect.include" value="com.strongjoshua.console.CommandExecutor" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.scenes.scene2d.ui.Table" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.scenes.scene2d.ui.Label" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.scenes.scene2d.ui.Window" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.scenes.scene2d.ui.TextArea" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.scenes.scene2d.ui.TextButton" />
	<extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.scenes.scene2d.ui.ScrollPane" />

	<extend-configuration-property name="gdx.files.classpath" value="default_skin/uiskin.json" />
	<extend-configuration-property name="gdx.files.classpath" value="default_skin/uiskin.atlas" />
	<extend-configuration-property name="gdx.files.classpath" value="default_skin/uiskin.png" />
	<extend-configuration-property name="gdx.files.classpath" value="default_skin/default.fnt" />
</module>
